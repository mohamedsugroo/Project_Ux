<p id="notice"><%= notice %></p>

<h1>Ltypes</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @ltypes.each do |ltype| %>
      <tr>
        <td><%= ltype.name %></td>
        <td><%= link_to 'Show', ltype %></td>
        <td><%= link_to 'Edit', edit_ltype_path(ltype) %></td>
        <td><%= link_to 'Destroy', ltype, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Ltype', new_ltype_path %>
